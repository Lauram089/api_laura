<%= form_for @usuarios do |f| %>

    <div>
    <%= f.label :alias %>
    <%= f.text_field :alias %>

    </div>

    <div>
    <%= f.label :nombre %>
    <%= f.text_field :nombre %>
    
    </div>

    <div>
    <%= f.label :curso %>
    <%= f.number_field :curso %>
    
    </div>

    <div>
    <%= f.label :area %>
    <%= f.text_field :area %>
    
    </div>

    <div>
    <%= f.submit%>
    
    
    </div>

    <% end %>
